<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/">
	<ShortName>{% block shortname %}{% endblock %}</ShortName>
	<LongName>{% block longname %}{% endblock %}</LongName>
	<Description>{% block description %}{% endblock %}</Description>
	<Tags>{% block tags %}{% endblock %}</Tags>
	<Image height="16" width="16" type="image/png">data:image/png;base64,</Image>
	<InputEncoding>UTF-8</InputEncoding>
	<OutputEncoding>UTF-8</OutputEncoding>
	<Url type="text/html" method="get" template="{% block url %}{% endblock %}"/>
	<Query role="example" searchTerms="{% block example %}{% endblock %}"/>
	<Language>en-us</Language>
</OpenSearchDescription>
